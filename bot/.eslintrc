{
	"env": {
		"node": true,
		"es2021": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:prettier/recommended"
	],
	"parserOptions": {
		"ecmaVersion": 2022,
		"sourceType": "module"
	},
	"plugins": [
		"prettier"
	],
	"rules": {
		"prettier/prettier": "error",
		"no-prototype-builtins": "off",
		"prefer-const": "error"
	}
}
